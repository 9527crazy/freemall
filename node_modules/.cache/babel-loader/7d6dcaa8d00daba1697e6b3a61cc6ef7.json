NavFooter from \"../components/Footer\";\nexport default {\n  name: \"cart\",\n  data: function data() {\n    return {\n      modalConfirm: false,\n      //弹框显示属性\n      delItem: '',\n      //准备删除的对象\n      cartList: []\n    };\n  },\n  components: {\n    NavHeader: NavHeader,\n    NavFooter: NavFooter,\n    Modal: Modal\n  },\n  computed: {\n    //当数组中所有对象都返回true的时候，我们整体才返回true\n    checkAllFlag: function checkAllFlag() {\n      return this.cartList.every(function (item) {\n        return item.checked;\n      });\n    },\n    //计算总金额\n    totalPrice: function totalPrice() {\n      var money = 0;\n      this.cartList.forEach(function (item) {\n        if (item.checked) {\n          money += item.productPrice * item.productNum;\n        }\n      });\n      return money;\n    },\n    //判断购物车是否有选中的商品\n    checkedCount: function checkedCount() {\n      return this.cartList.some(function (item) {\n        return item.checked;\n      });\n    }\n  },\n  filters: {\n    capitalize: function capitalize(value) {\n      if (!value) return 0.00;\n      return '￥' + value.toFixed(2) + '元';\n    }\n  },\n  mounted: function mounted() {\n    this.init(); //初始化购物车列表\n  },\n  methods: {\n    //初始化购物车列表数据\n    init: function init() {\n      var _this = this;\n\n      this.axios.get(\"/mock/cart.json\").then(function (response) {\n        var res = response.data;\n        _this.cartList = res.data;\n      });\n    },\n    //修改购物车数量\n    editCart: function editCart(type, item) {\n      if (type == 'add') {\n        item.productNum++;\n      }\n\n      if (type == 'minus' && item.productNum > 0) {\n        item.productNum--;\n      }\n\n      if (type == 'checked') {\n        item.checked = !item.checked;\n      }\n    },\n    //删除数据确认弹框\n    delCartConfirm: function delCartConfirm(item) {\n      this.delItem = item;\n      this.modalConfirm = true;\n    },\n    closeModal: function closeModal() {\n      this.modalConfirm = false;\n    },\n    //删除购物车数据\n    delCart: function delCart() {\n      var _this2 = this;\n\n      var delItem = this.delItem;\n      this.cartList.forEach(function (item, index) {\n        if (delItem.productId == item.productId) {\n          _this2.cartList.splice(index, 1);\n\n          _this2.modalConfirm = false;\n        }\n      });\n    },\n    //全选和反选\n    toggleCheckAll: function toggleCheckAll() {\n      var flag = !this.checkAllFlag;\n      this.cartList.forEach(function (item) {\n        item.checked = flag;\n      });\n    },\n    checkOut: function checkOut() {\n      if (this.checkedCount) {\n        this.$router.push({});\n      }\n    }\n  }\n};",{"version":3,"sources":["cart.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8IA,OAAA,SAAA;AACA,OAAA,KAAA;AACA,OAAA,SAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,MADA;AAEA,EAAA,IAFA,kBAEA;AACA,WAAA;AACA,MAAA,YAAA,EAAA,KADA;AACA;AACA,MAAA,OAAA,EAAA,EAFA;AAEA;AACA,MAAA,QAAA,EAAA;AAHA,KAAA;AAKA,GARA;AASA,EAAA,UAAA,EAAA;AACA,IAAA,SAAA,EAAA,SADA;AAEA,IAAA,SAAA,EAAA,SAFA;AAGA,IAAA,KAAA,EAAA;AAHA,GATA;AAcA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,YAFA,0BAEA;AACA,aAAA,KAAA,QAAA,CAAA,KAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,OAAA;AACA,OAFA,CAAA;AAGA,KANA;AAOA;AACA,IAAA,UARA,wBAQA;AACA,UAAA,KAAA,GAAA,CAAA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,OAAA,EAAA;AACA,UAAA,KAAA,IAAA,IAAA,CAAA,YAAA,GAAA,IAAA,CAAA,UAAA;AACA;AACA,OAJA;AAKA,aAAA,KAAA;AACA,KAhBA;AAiBA;AACA,IAAA,YAlBA,0BAkBA;AACA,aAAA,KAAA,QAAA,CAAA,IAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,OAAA;AACA,OAFA,CAAA;AAGA;AAtBA,GAdA;AAsCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,KADA,EACA;AACA,UAAA,CAAA,KAAA,EAAA,OAAA,IAAA;AACA,aAAA,MAAA,KAAA,CAAA,OAAA,CAAA,CAAA,CAAA,GAAA,GAAA;AACA;AAJA,GAtCA;AA4CA,EAAA,OA5CA,qBA4CA;AACA,SAAA,IAAA,GADA,CACA;AACA,GA9CA;AA+CA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,IAFA,kBAEA;AAAA;;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,iBAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,GAAA,GAAA,QAAA,CAAA,IA